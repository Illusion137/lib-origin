{
    "compilerOptions": {
        "strict": true,
        "noImplicitAny": true,
        "noEmitOnError": true,
        "noEmit": true,
        "module": "commonjs",
        "esModuleInterop": true,
        "target": "es2019",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "sourceMap": true,
        "outDir": "dist",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "allowUnreachableCode": false,
        "alwaysStrict": true,
        "noFallthroughCasesInSwitch": true,
        "strictNullChecks": true,
        "noImplicitReturns": true,
        "skipLibCheck": true
    },
    "exclude": ["node_modules/**", "origin/src/youtube_dl/**"],
    "lib": [
        "es2015"
    ]
}