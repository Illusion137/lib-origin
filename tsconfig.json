{
    "compilerOptions": {
      "strict": true,
      "noImplicitAny": true,
      "noEmitOnError": true,
      "noEmit": true,
      "module": "commonjs",
      "esModuleInterop": true,
      "target": "es6",
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "sourceMap": true,
      "outDir": "dist"
    },
    "lib": ["es2015"]
}